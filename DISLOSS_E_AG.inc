PARAMETER DISLOSS_E_AG(AAA,GGG)  'Loss in electricity distribution associated to specific technology in a particular area';
*Source for offshore losses: https://www.sciencedirect.com/science/article/pii/S0378779605002609
DISLOSS_E_AG(IA,G)$(GDATA(G,'GDTECHGROUP') EQ WINDTURBINE_OFFSHORE AND (AGKN(IA,G) OR SUM(Y, GKFX(Y,IA,G))))=0.1;
DISLOSS_E_AG(IA,G)$(GDATA(G,'GDSUBTECHGROUP') EQ RG2_OFF1 AND (AGKN(IA,G) OR SUM(Y, GKFX(Y,IA,G))))=0.2;
DISLOSS_E_AG(IA,G)$(GDATA(G,'GDSUBTECHGROUP') EQ RG2_OFF2 AND (AGKN(IA,G) OR SUM(Y, GKFX(Y,IA,G))))=0.2;
DISLOSS_E_AG(IA,G)$(GDATA(G,'GDSUBTECHGROUP') EQ RG2_OFF3 AND (AGKN(IA,G) OR SUM(Y, GKFX(Y,IA,G))))=0.2;
DISLOSS_E_AG(IA,G)$(GDATA(G,'GDSUBTECHGROUP') EQ RG2_OFF4 AND (AGKN(IA,G) OR SUM(Y, GKFX(Y,IA,G))))=0.2;
DISLOSS_E_AG(IA,G)$(GDATA(G,'GDSUBTECHGROUP') EQ RG2_OFF5 AND (AGKN(IA,G) OR SUM(Y, GKFX(Y,IA,G))))=0.2;
